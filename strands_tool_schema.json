{
  "openapi": "3.0.0",
  "info": {
    "title": "Strands ReAct Search",
    "version": "1.0.0",
    "description": "Advanced ReAct search tool"
  },
  "paths": {
    "/search": {
      "post": {
        "summary": "Perform advanced search",
        "description": "Use this when default search fails to find relevant information",
        "operationId": "strandsReactSearch",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "query": {
                    "type": "string",
                    "description": "Search query"
                  }
                },
                "required": ["query"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Search results",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "string",
                      "description": "Search result"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}